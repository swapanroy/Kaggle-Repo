    "# Extract the Embedding Space to visualize the Goldilocks clusters\n",
    "embedding_model = models.Model(inputs=model.input, outputs=model.get_layer('embedding_space').output)\n",
    "\n",
    "# Get a batch of data\n",
    "(test_imgs, test_clins), test_labels = next(train_multi_gen)\n",
    "embeddings = embedding_model.predict([test_imgs, test_clins])\n",
    "\n",
    "# Run t-SNE\n",
    "tsne = TSNE(n_components=2, random_state=42)\n",
    "vis_dims = tsne.fit_transform(embeddings)\n",
    "\n",
    "# Plot the map\n",
    "plt.figure(figsize=(10, 7))\n",
    "plt.scatter(vis_dims[:, 0], vis_dims[:, 1], c=np.argmax(test_labels, axis=1), cmap='viridis')\n",
